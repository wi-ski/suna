# Copy this file to .env and fill in your values

# Environment Mode
# Valid values: local, staging, production
ENV_MODE=local

# Server Configuration
PORT=8000
HOST=0.0.0.0
LOG_LEVEL=info

# Database
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Redis (Optional for caching)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_SSL=false

# RabbitMQ (Optional for queuing)
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672

# LLM Providers
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
MODEL_TO_USE=anthropic/claude-3-sonnet-20240229

# AWS (Optional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION_NAME=

# Additional LLM Providers
GROQ_API_KEY=
OPENROUTER_API_KEY=

# Data APIs
RAPID_API_KEY=

# Web Search
TAVILY_API_KEY=

# Web Scraping
FIRECRAWL_API_KEY=
FIRECRAWL_URL=

# Sandbox Container Provider
DAYTONA_API_KEY=
DAYTONA_SERVER_URL=
DAYTONA_TARGET=

# Observability
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=https://cloud.langfuse.com

# Additional Services
SMITHERY_API_KEY=
